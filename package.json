{
  "name": "graphErc721",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "create-local": "graph create --node http://127.0.0.1:8020/ testnetPolymonBondingCurveMons",
    "deploy-local:pmon": "yarn codegen && yarn build && yarn prepare:pmon && graph deploy --node http://127.0.0.1:8020/ --ipfs http://127.0.0.1:5001 testnetPolymonBondingCurveMons",
    "remove-local": "graph remove --node http://127.0.0.1:8020/ testnetPolymonBondingCurveMons",
    "prepare:pmon": "mustache config/pmon-testnet.json subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "mustache": "^4.2.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  },
